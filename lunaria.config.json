{
  "$schema": "./node_modules/@lunariajs/core/config.schema.json",
  "repository": {
    "name": "aptos-labs/aptos-docs"
  },
  "files": [
    {
      "location": "src/content/docs/**/*.{md,mdx}",
      "pattern": "src/content/docs/@lang/@path",
      "type": "universal"
    }
  ],
  "locales": [
    {
      "label": "简体中文",
      "lang": "zh"
    },
    {
      "label": "日本語",
      "lang": "ja"
    }
  ],
  "defaultLocale": {
    "label": "English",
    "lang": "en"
  }
}
