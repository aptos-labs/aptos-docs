{
  "name": "aptos-docs-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "migrate": "cd scripts/migration && pnpm install && pnpm build && pnpm migrate",
    "audit-sidebar": "tsx scripts/audit-sidebar.ts",
    "postmigrate": "pnpm format:content",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "prepush": "astro check --noSync",
    "sync": "astro sync",
    "lint": "pnpm run --parallel --filter='.' /^lint:*/",
    "lint:eslint": "eslint . --cache --cache-location=.astro/eslint/.cache",
    "lint:prettier": "prettier --check .",
    "format": "pnpm run --parallel --filter='.' /^format:*/",
    "format:eslint": "pnpm run lint --fix",
    "format:prettier": "prettier . --write",
    "format:content": "remark \"src/content/**/*.mdx\" --output --use remark-mdx",
    "postinstall": "pnpm exec simple-git-hooks"
  },
  "engines": {
    "node": "22.x",
    "pnpm": "^10.2.0"
  },
  "packageManager": "pnpm@10.2.0",
  "dependencies": {
    "@aptos-labs/ai-chatbot-client": "^0.1.3",
    "@astrojs/markdown-remark": "^6.3.6",
    "@astrojs/node": "^9.4.2",
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/react": "^4.3.0",
    "@astrojs/sitemap": "^3.5.0",
    "@astrojs/starlight": "^0.35.2",
    "@astrojs/starlight-docsearch": "^0.6.0",
    "@astrojs/starlight-tailwind": "^4.0.1",
    "@astrojs/tailwind": "^6.0.2",
    "@astrojs/vercel": "^8.2.6",
    "@expressive-code/plugin-collapsible-sections": "^0.41.3",
    "@graphiql/toolkit": "^0.11.3",
    "@iconify/json": "^2.2.360",
    "@nanostores/react": "^1.0.0",
    "@octokit/graphql": "^9.0.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@readme/openapi-parser": "^4.1.2",
    "@scalar/api-client": "^2.5.19",
    "@scalar/snippetz": "^0.3.1",
    "@tailwindcss/vite": "^4.1.11",
    "@types/gtag.js": "^0.0.20",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vercel/analytics": "^1.5.0",
    "@vercel/edge": "^1.2.2",
    "@vercel/og": "^0.6.8",
    "@vercel/speed-insights": "^1.2.0",
    "astro": "^5.13.2",
    "astro-embed": "^0.9.0",
    "astro-favicons": "^3.1.5",
    "astro-icon": "^1.1.5",
    "astro-seo-schema": "^5.0.0",
    "class-variance-authority": "^0.7.1",
    "firebase": "^11.10.0",
    "github-slugger": "^2.0.0",
    "graphiql": "^4.1.2",
    "gray-matter": "^4.0.3",
    "jose": "^6.0.12",
    "katex": "^0.16.22",
    "kleur": "^4.1.5",
    "lucide-react": "^0.532.0",
    "mdast-util-to-string": "^4.0.0",
    "nanostores": "^1.0.1",
    "octokit": "^5.0.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "satori-html": "^0.3.2",
    "schema-dts": "^1.1.5",
    "sharp": "^0.34.3",
    "shiki": "^3.8.1",
    "starlight-llms-txt": "^0.5.1",
    "starlight-openapi": "^0.13.0",
    "starlight-package-managers": "^0.11.0",
    "tailwindcss": "^4.1.11",
    "unist-util-remove": "^4.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@aptos-foundation/astro-vercel-middleware": "^0.1.0",
    "@astrojs/check": "^0.9.4",
    "@crowdin/cli": "^4.9.0",
    "@eslint/js": "^9.31.0",
    "@iconify-json/ph": "^1.2.2",
    "@swc/core": "^1.13.1",
    "@types/node": "^22.16.5",
    "@vercel/functions": "^3.0.0",
    "@vercel/node": "^5.3.6",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^9.31.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "fast-glob": "^3.3.3",
    "globals": "^16.3.0",
    "nano-staged": "^0.8.0",
    "openapi-types": "^12.1.3",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "remark-cli": "^12.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx": "^3.1.0",
    "simple-git-hooks": "^2.13.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^6.3.5"
  },
  "pnpm": {
    "patchedDependencies": {
      "starlight-openapi": "patches/starlight-openapi.patch",
      "starlight-llms-txt": "patches/starlight-llms-txt.patch"
    },
    "overrides": {
      "axios": ">=1.11.0",
      "vite": "^6.3.5",
      "esbuild": ">=0.25.0",
      "undici": ">=5.28.5",
      "path-to-regexp": ">=6.3.0",
      "glob": "^11",
      "devalue": ">=5.3.2",
      "prismjs": ">=1.30.0"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "./node_modules/.bin/nano-staged",
    "pre-push": "pnpm prepush"
  },
  "nano-staged": {
    "*.{js,cjs,mjs,jsx,ts,mts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/content/**/*.mdx": [
      "remark --output --use remark-mdx"
    ]
  }
}
