{
  "name": "aptos-docs-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "prebuild": "pnpm build:middleware-matcher && pnpm build:middleware",
    "predev": "pnpm build:middleware-matcher && pnpm build:middleware --watch &",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "pnpm prebuild && astro build && pnpm build:generate-middleware-function",
    "build:middleware-matcher": "node ./scripts/generate-middleware-matcher.js",
    "build:middleware": "node ./scripts/generate-middleware.js",
    "build:generate-middleware-function": "node ./scripts/generate-middleware-function.js",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "prepush": "astro check --noSync",
    "sync": "astro sync",
    "lint": "pnpm run --parallel --filter='.' /^lint:*/",
    "lint:eslint": "eslint . --cache --cache-location=.astro/eslint/.cache",
    "lint:prettier": "prettier --check .",
    "format": "pnpm run --parallel --filter='.' /^format:*/",
    "format:eslint": "pnpm run lint --fix",
    "format:prettier": "prettier . --write",
    "format:content": "remark \"src/content/**/*.{md,mdx}\" --output",
    "postinstall": "pnpm exec simple-git-hooks",
    "tools:audit-sidebar": "tsx scripts/audit-sidebar.ts",
    "tools:fix-i18n-links": "node ./scripts/fix-i18n-links/fix-i18n-links-launcher.cjs"
  },
  "engines": {
    "node": ">=22",
    "pnpm": "^10.2.0"
  },
  "packageManager": "pnpm@10.2.0",
  "dependencies": {
    "@aptos-labs/ai-chatbot-client": "^0.1.4",
    "@astrojs/node": "^9.5.0",
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/react": "^4.4.0",
    "@astrojs/sitemap": "^3.6.0",
    "@astrojs/starlight": "^0.36.1",
    "@astrojs/starlight-docsearch": "^0.6.0",
    "@astrojs/starlight-tailwind": "^4.0.1",
    "@astrojs/tailwind": "^6.0.2",
    "@astrojs/vercel": "^8.2.10",
    "@iconify/json": "^2.2.397",
    "@nanostores/react": "^1.0.0",
    "@octokit/graphql": "^9.0.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@vercel/analytics": "^1.5.0",
    "@vercel/edge": "^1.2.2",
    "@vercel/og": "^0.8.5",
    "@vercel/speed-insights": "^1.2.0",
    "astro": "^5.14.6",
    "astro-mermaid": "^1.1.0",
    "class-variance-authority": "^0.7.1",
    "firebase": "^12.4.0",
    "jose": "^6.1.0",
    "lucide-react": "^0.546.0",
    "mermaid": "^11.12.0",
    "nanostores": "^1.0.1",
    "octokit": "^5.0.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.6",
    "starlight-image-zoom": "^0.13.0",
    "starlight-links-validator": "^0.19.0",
    "tailwindcss": "^4.1.14"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/markdown-remark": "^6.3.8",
    "@crowdin/cli": "^4.11.0",
    "@eslint/js": "^9.38.0",
    "@expressive-code/plugin-collapsible-sections": "^0.41.3",
    "@graphiql/toolkit": "^0.11.3",
    "@iconify-json/ph": "^1.2.2",
    "@readme/openapi-parser": "^5.0.2",
    "@scalar/api-client": "^2.8.1",
    "@scalar/snippetz": "^0.4.11",
    "@swc/core": "^1.13.5",
    "@tailwindcss/vite": "^4.1.14",
    "@types/gtag.js": "^0.0.20",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vercel/node": "^5.4.1",
    "astro-embed": "^0.9.1",
    "astro-eslint-parser": "^1.2.2",
    "astro-favicons": "^3.1.5",
    "astro-icon": "^1.1.5",
    "astro-seo-schema": "^5.1.0",
    "eslint": "^9.38.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "fast-glob": "^3.3.3",
    "github-slugger": "^2.0.0",
    "globals": "^16.4.0",
    "graphiql": "^5.2.0",
    "gray-matter": "^4.0.3",
    "katex": "^0.16.25",
    "kleur": "^4.1.5",
    "mdast-util-to-string": "^4.0.0",
    "nano-staged": "^0.8.0",
    "openapi-types": "^12.1.3",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-cli": "^12.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-mdx": "^3.1.1",
    "satori-html": "^0.3.2",
    "schema-dts": "^1.1.5",
    "sharp": "^0.34.4",
    "shiki": "^3.13.0",
    "simple-git-hooks": "^2.13.1",
    "starlight-llms-txt": "^0.6.0",
    "starlight-openapi": "^0.20.0",
    "starlight-package-managers": "^0.11.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "unist-util-remove": "^4.0.0",
    "unist-util-visit": "^5.0.0",
    "vite": "^7.1.10",
    "vite-plugin-monaco-editor": "^1.1.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "starlight-openapi": "patches/starlight-openapi.patch",
      "astro": "patches/astro-csp-patch.patch",
      "@astrojs/vercel": "patches/@astrojs__vercel.patch"
    },
    "overrides": {
      "vite": "^7.1.10",
      "esbuild": ">=0.25.0",
      "undici": ">=5.28.5",
      "path-to-regexp": ">=6.3.0",
      "glob": "^11",
      "devalue": ">=5.3.2",
      "prismjs": ">=1.30.0"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "./node_modules/.bin/nano-staged",
    "pre-push": "pnpm prepush"
  },
  "nano-staged": {
    "*.{js,cjs,mjs,jsx,ts,mts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/content/**/*.mdx": []
  }
}
