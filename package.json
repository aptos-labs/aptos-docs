{
  "name": "aptos-docs-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "prebuild": "pnpm build:middleware-matcher && pnpm build:middleware",
    "predev": "pnpm build:middleware-matcher && pnpm build:middleware --watch &",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "pnpm prebuild && astro build && pnpm build:generate-middleware-function",
    "build:middleware-matcher": "tsx ./scripts/generate-middleware-matcher.js",
    "build:middleware": "tsx ./scripts/generate-middleware.js",
    "build:generate-middleware-function": "tsx ./scripts/generate-middleware-function.js",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "prepush": "astro check --noSync",
    "sync": "astro sync",
    "lint": "pnpm run --parallel --filter='.' /^lint:*/",
    "lint:biome": "biome check .",
    "lint:prettier": "prettier --check \"**/*.astro\"",
    "format": "pnpm run --parallel --filter='.' /^format:*/",
    "format:biome": "biome check --fix .",
    "format:prettier": "prettier --write \"**/*.astro\"",
    "format:content": "remark \"src/content/**/*.{md,mdx}\" --output",
    "postinstall": "pnpm exec simple-git-hooks",
    "tools:audit-sidebar": "tsx scripts/audit-sidebar.ts",
    "tools:fix-i18n-links": "tsx ./scripts/fix-i18n-links/fix-i18n-links-launcher.cjs",
    "test": "vitest run",
    "test:mermaid": "vitest run tests/mermaid-rendering.test.ts"
  },
  "engines": {
    "node": ">=22",
    "pnpm": "^10.30.3"
  },
  "packageManager": "pnpm@10.30.3",
  "dependencies": {
    "@aptos-labs/ai-chatbot-client": "^0.1.4",
    "@astrojs/node": "^9.5.4",
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/react": "^4.4.2",
    "@astrojs/sitemap": "^3.7.0",
    "@astrojs/starlight": "^0.37.6",
    "@astrojs/starlight-docsearch": "^0.6.1",
    "@astrojs/starlight-tailwind": "^4.0.2",
    "@astrojs/tailwind": "^6.0.2",
    "@astrojs/vercel": "^9.0.4",
    "@iconify/json": "^2.2.443",
    "@nanostores/react": "^1.0.0",
    "@octokit/graphql": "^9.0.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@vercel/analytics": "^1.6.1",
    "@vercel/edge": "^1.2.2",
    "@vercel/og": "^0.9.0",
    "@vercel/speed-insights": "^1.3.1",
    "astro": "^5.18.0",
    "astro-mermaid": "^1.3.1",
    "class-variance-authority": "^0.7.1",
    "firebase": "^12.10.0",
    "jose": "^6.1.3",
    "lucide-react": "^0.575.0",
    "mermaid": "^11.12.3",
    "nanostores": "^1.1.1",
    "octokit": "^5.0.5",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "starlight-image-zoom": "^0.13.2",
    "starlight-links-validator": "^0.19.2",
    "tailwindcss": "^4.2.1"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/markdown-remark": "^6.3.10",
    "@biomejs/biome": "2.4.4",
    "@crowdin/cli": "^4.14.0",
    "@expressive-code/plugin-collapsible-sections": "^0.41.7",
    "@graphiql/toolkit": "^0.11.3",
    "@iconify-json/ph": "^1.2.2",
    "@readme/openapi-parser": "^5.5.0",
    "@scalar/api-client": "^2.29.3",
    "@scalar/snippetz": "^0.6.16",
    "@swc/core": "^1.15.13",
    "@tailwindcss/vite": "^4.2.1",
    "@types/gtag.js": "^0.0.20",
    "@types/node": "^25.3.1",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vercel/node": "^5.6.7",
    "astro-embed": "^0.12.0",
    "astro-favicons": "^3.1.5",
    "astro-icon": "^1.1.5",
    "astro-seo-schema": "^5.2.0",
    "baseline-browser-mapping": "^2.10.0",
    "fast-glob": "^3.3.3",
    "github-slugger": "^2.0.0",
    "graphiql": "^5.2.2",
    "gray-matter": "^4.0.3",
    "katex": "^0.16.33",
    "kleur": "^4.1.5",
    "mdast-util-to-string": "^4.0.0",
    "nano-staged": "^0.9.0",
    "openapi-types": "^12.1.3",
    "prettier": "^3.8.1",
    "prettier-plugin-astro": "^0.14.1",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-cli": "^12.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-mdx": "^3.1.1",
    "satori-html": "^0.3.2",
    "schema-dts": "^1.1.5",
    "sharp": "^0.34.5",
    "shiki": "^3.23.0",
    "simple-git-hooks": "^2.13.1",
    "starlight-llms-txt": "^0.7.0",
    "starlight-openapi": "^0.22.0",
    "starlight-package-managers": "^0.12.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "unist-util-remove": "^4.0.0",
    "unist-util-visit": "^5.1.0",
    "vite": "^6.4.1",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "patchedDependencies": {
      "starlight-openapi": "patches/starlight-openapi.patch",
      "astro": "patches/astro.patch",
      "@astrojs/vercel": "patches/@astrojs__vercel.patch",
      "vite-plugin-monaco-editor@1.1.0": "patches/vite-plugin-monaco-editor@1.1.0.patch"
    },
    "overrides": {
      "minimatch@^10": "^10.2.1",
      "undici@^5||^6": "^6.23.0",
      "lodash@4": "^4.17.23",
      "ajv@^8": "^8.18.0"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "./node_modules/.bin/nano-staged",
    "pre-push": "pnpm prepush"
  },
  "nano-staged": {
    "*.{js,cjs,mjs,jsx,ts,mts,tsx,json}": [
      "biome check --fix --no-errors-on-unmatched"
    ],
    "*.astro": [
      "prettier --write"
    ],
    "src/content/**/*.mdx": [
      "remark \"src/content/**/*.{md,mdx}\" --output"
    ]
  }
}
