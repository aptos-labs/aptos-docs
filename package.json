{
  "name": "aptos-docs-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "migrate": "cd scripts/migration && pnpm install && pnpm build && pnpm migrate",
    "postmigrate": "pnpm format:content",
    "prebuild": "pnpm build:middleware-matcher && pnpm build:middleware",
    "predev": "pnpm build:middleware-matcher && pnpm build:middleware --watch &",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "pnpm prebuild && astro build && pnpm build:generate-middleware-function",
    "build:middleware-matcher": "node ./scripts/generate-middleware-matcher.js",
    "build:middleware": "node ./scripts/generate-middleware.js",
    "build:generate-middleware-function": "node ./scripts/generate-middleware-function.js",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "sync": "astro sync",
    "lint": "pnpm run --parallel --filter='.' /^lint:*/",
    "lint:eslint": "eslint . --cache --cache-location=.astro/eslint/.cache",
    "lint:prettier": "prettier --check .",
    "format": "pnpm run --parallel --filter='.' /^format:*/",
    "format:eslint": "pnpm run lint --fix",
    "format:prettier": "prettier . --write",
    "format:content": "remark \"src/content/**/*.mdx\" --output --use remark-mdx",
    "postinstall": "pnpm exec simple-git-hooks"
  },
  "engines": {
    "node": "22.x",
    "pnpm": "^10.2.0"
  },
  "packageManager": "pnpm@10.2.0",
  "dependencies": {
    "@astrojs/markdown-remark": "^6.3.1",
    "@astrojs/node": "^9.1.3",
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/react": "^4.2.3",
    "@astrojs/sitemap": "^3.3.0",
    "@astrojs/starlight": "^0.32.6",
    "@astrojs/starlight-docsearch": "^0.6.0",
    "@astrojs/starlight-tailwind": "^3.0.1",
    "@astrojs/tailwind": "^6.0.2",
    "@astrojs/vercel": "^8.1.3",
    "@expressive-code/plugin-collapsible-sections": "^0.40.2",
    "@graphiql/toolkit": "^0.11.1",
    "@iconify/json": "^2.2.324",
    "@nanostores/react": "^0.8.4",
    "@octokit/graphql": "^8.2.1",
    "@readme/openapi-parser": "^2.7.0",
    "@scalar/api-client": "^2.3.12",
    "@scalar/snippetz": "^0.2.19",
    "@tailwindcss/vite": "^4.1.3",
    "@types/gtag.js": "^0.0.20",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@vercel/analytics": "^1.5.0",
    "@vercel/edge": "^1.2.1",
    "@vercel/og": "^0.6.8",
    "@vercel/speed-insights": "^1.2.0",
    "astro": "^5.6.1",
    "astro-embed": "^0.9.0",
    "astro-favicons": "^3.1.5",
    "astro-icon": "^1.1.5",
    "astro-seo-schema": "^5.0.0",
    "class-variance-authority": "^0.7.1",
    "firebase": "^11.6.0",
    "github-slugger": "^2.0.0",
    "graphiql": "^3.8.3",
    "gray-matter": "^4.0.3",
    "jose": "^6.0.10",
    "katex": "^0.16.21",
    "kleur": "^4.1.5",
    "mdast-util-to-string": "^4.0.0",
    "nanostores": "^0.11.4",
    "octokit": "^4.1.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-math": "^6.0.0",
    "satori-html": "^0.3.2",
    "schema-dts": "^1.1.5",
    "sharp": "^0.33.5",
    "shiki": "^2.5.0",
    "starlight-llms-txt": "^0.5.0",
    "starlight-openapi": "^0.13.0",
    "starlight-package-managers": "^0.10.0",
    "tailwindcss": "^4.1.3",
    "unist-util-remove": "^4.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@crowdin/cli": "^4.7.0",
    "@eslint/js": "^9.24.0",
    "@iconify-json/ph": "^1.2.2",
    "@swc/core": "^1.11.18",
    "@types/node": "^22.14.0",
    "@vercel/node": "^5.1.14",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^9.24.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^3.10.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-import-x": "^4.10.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "fast-glob": "^3.3.3",
    "globals": "^15.15.0",
    "nano-staged": "^0.8.0",
    "openapi-types": "^12.1.3",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "remark-cli": "^12.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx": "^3.1.0",
    "simple-git-hooks": "^2.12.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "vite": "^6.2.5"
  },
  "pnpm": {
    "patchedDependencies": {
      "starlight-openapi": "patches/starlight-openapi.patch",
      "@scalar/api-client": "patches/@scalar__api-client.patch",
      "starlight-llms-txt@0.5.0": "patches/starlight-llms-txt@0.5.0.patch",
      "@vercel/node": "patches/@vercel__node.patch"
    },
    "overrides": {
      "vite": "^6",
      "esbuild": ">=0.25.0",
      "undici": ">=5.28.5",
      "path-to-regexp": ">=6.3.0"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "./node_modules/.bin/nano-staged",
    "pre-push": "astro check --noSync"
  },
  "nano-staged": {
    "*.{js,cjs,mjs,jsx,ts,mts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/content/**/*.mdx": [
      "remark --output --use remark-mdx"
    ]
  }
}
