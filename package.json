{
  "name": "aptos-docs-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "prebuild": "pnpm build:middleware-matcher && pnpm build:middleware",
    "predev": "pnpm build:middleware-matcher && pnpm build:middleware --watch &",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "pnpm prebuild && astro build && pnpm build:generate-middleware-function",
    "build:middleware-matcher": "node ./scripts/generate-middleware-matcher.js",
    "build:middleware": "node ./scripts/generate-middleware.js",
    "build:generate-middleware-function": "node ./scripts/generate-middleware-function.js",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "prepush": "astro check --noSync",
    "sync": "astro sync",
    "lint": "pnpm run --parallel --filter='.' /^lint:*/",
    "lint:eslint": "eslint . --cache --cache-location=.astro/eslint/.cache",
    "lint:prettier": "prettier --check .",
    "format": "pnpm run --parallel --filter='.' /^format:*/",
    "format:eslint": "pnpm run lint --fix",
    "format:prettier": "prettier . --write",
    "format:content": "remark \"src/content/**/*.{md,mdx}\" --output",
    "postinstall": "pnpm exec simple-git-hooks",
    "tools:audit-sidebar": "tsx scripts/audit-sidebar.ts",
    "tools:fix-i18n-links": "node ./scripts/fix-i18n-links/fix-i18n-links-launcher.cjs"
  },
  "engines": {
    "node": ">=22",
    "pnpm": "^10.2.0"
  },
  "packageManager": "pnpm@10.2.0",
  "dependencies": {
    "@aptos-labs/ai-chatbot-client": "^0.1.4",
    "@astrojs/node": "^9.5.1",
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/react": "^4.4.2",
    "@astrojs/sitemap": "^3.6.0",
    "@astrojs/starlight": "^0.37.1",
    "@astrojs/starlight-docsearch": "^0.6.1",
    "@astrojs/starlight-tailwind": "^4.0.2",
    "@astrojs/tailwind": "^6.0.2",
    "@astrojs/vercel": "^9.0.2",
    "@iconify/json": "^2.2.425",
    "@nanostores/react": "^1.0.0",
    "@octokit/graphql": "^9.0.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@vercel/analytics": "^1.6.1",
    "@vercel/edge": "^1.2.2",
    "@vercel/og": "^0.8.6",
    "@vercel/speed-insights": "^1.3.1",
    "astro": "^5.16.6",
    "astro-mermaid": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "firebase": "^12.7.0",
    "jose": "^6.1.3",
    "lucide-react": "^0.562.0",
    "mermaid": "^11.12.2",
    "nanostores": "^1.1.0",
    "octokit": "^5.0.5",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "starlight-image-zoom": "^0.13.2",
    "starlight-links-validator": "^0.19.2",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/markdown-remark": "^6.3.10",
    "@crowdin/cli": "^4.12.0",
    "@eslint/js": "^9.39.2",
    "@expressive-code/plugin-collapsible-sections": "^0.41.5",
    "@graphiql/toolkit": "^0.11.3",
    "@iconify-json/ph": "^1.2.2",
    "@readme/openapi-parser": "^5.4.0",
    "@scalar/api-client": "^2.17.1",
    "@scalar/snippetz": "^0.6.1",
    "@swc/core": "^1.15.8",
    "@tailwindcss/vite": "^4.1.18",
    "@types/gtag.js": "^0.0.20",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vercel/node": "^5.5.16",
    "astro-embed": "^0.10.0",
    "astro-eslint-parser": "^1.2.2",
    "astro-favicons": "^3.1.5",
    "astro-icon": "^1.1.5",
    "astro-seo-schema": "^5.1.0",
    "baseline-browser-mapping": "^2.9.11",
    "eslint": "^9.39.2",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "fast-glob": "^3.3.3",
    "github-slugger": "^2.0.0",
    "globals": "^17.0.0",
    "graphiql": "^5.2.2",
    "gray-matter": "^4.0.3",
    "katex": "^0.16.27",
    "kleur": "^4.1.5",
    "mdast-util-to-string": "^4.0.0",
    "nano-staged": "^0.9.0",
    "openapi-types": "^12.1.3",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.1",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-cli": "^12.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-mdx": "^3.1.1",
    "satori-html": "^0.3.2",
    "schema-dts": "^1.1.5",
    "sharp": "^0.34.5",
    "shiki": "^3.20.0",
    "simple-git-hooks": "^2.13.1",
    "starlight-llms-txt": "^0.7.0",
    "starlight-openapi": "^0.21.1",
    "starlight-package-managers": "^0.11.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "unist-util-remove": "^4.0.0",
    "unist-util-visit": "^5.0.0",
    "vite": "^7.3.0",
    "vite-plugin-monaco-editor": "^1.1.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "starlight-openapi": "patches/starlight-openapi.patch",
      "astro": "patches/astro.patch",
      "@astrojs/vercel": "patches/@astrojs__vercel.patch",
      "vite-plugin-monaco-editor@1.1.0": "patches/vite-plugin-monaco-editor@1.1.0.patch"
    },
    "overrides": {
      "@anthropic-ai/claude-code": ">=2.0.31",
      "vite": "^7.3.0",
      "esbuild": ">=0.25.0",
      "undici": ">=5.28.5",
      "path-to-regexp": ">=6.3.0",
      "glob": ">=11.1.0",
      "devalue": ">=5.3.2",
      "prismjs": ">=1.30.0",
      "sitemap": ">=8.0.2",
      "tar": ">=7.5.2",
      "js-yaml": ">=4.1.1",
      "mdast-util-to-hast": ">=13.2.1",
      "sharp": ">=0.34.0"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "./node_modules/.bin/nano-staged",
    "pre-push": "pnpm prepush"
  },
  "nano-staged": {
    "*.{js,cjs,mjs,jsx,ts,mts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/content/**/*.mdx": []
  }
}
