---
import { getCollection } from "astro:content";
import StarlightPage from "@astrojs/starlight/components/StarlightPage.astro";
import { CardGrid, LinkCard } from "@astrojs/starlight/components";

const moveReferenceEntries = await getCollection("moveReference");
---

<StarlightPage frontmatter={{ title: "Move Reference" }}>
  <CardGrid>
    {
      moveReferenceEntries.map((entry) => (
        <LinkCard href={`/move-reference/${entry.id}`} title={entry.data.title || entry.id} />
      ))
    }
  </CardGrid>
</StarlightPage>
